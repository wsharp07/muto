{
  "name": "muto",
  "version": "0.0.0",
  "description": "Simple database migration tool",
  "author": "William Sharp @wsharp07",
  "bin": "./bin/run",
  "homepage": "https://github.com/wsharp07/muto",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "wsharp07/muto",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@databases/mysql": "^6.0.0",
    "@databases/pg": "^5.4.1",
    "@oclif/core": "^2.9.4",
    "@oclif/plugin-help": "^5.2.14",
    "@oclif/plugin-plugins": "^3.1.8"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.31",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unicorn": "^48.0.0",
    "husky": "^8.0.0",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.3",
    "memfs": "^4.2.0",
    "oclif": "^3.10.0",
    "pinst": "^3.0.0",
    "prettier": "3.0.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  },
  "oclif": {
    "bin": "muto",
    "dirname": "muto",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "migrate": {
        "description": "Simple database migration tool"
      }
    }
  },
  "scripts": {
    "dev": "./bin/dev",
    "build": "shx rm -rf dist && tsc -b && tsc-alias -p tsconfig.json",
    "lint": "eslint .",
    "postpack": "shx rm -f oclif.manifest.json && pinst --enable",
    "oclif-paths": "ts-node --project tsconfig.json -r tsconfig-paths/register node_modules/.bin/oclif",
    "prepack": "npm run build && npm run oclif-paths manifest && npm run oclif-paths readme",
    "test": "jest",
    "version": "oclif readme && git add README.md",
    "postinstall": "husky install"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bugs": "https://github.com/wsharp07/muto/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "packageManager": "yarn@3.6.1"
}
