{
  "name": "muto",
  "version": "0.0.0",
  "description": "Simple database migration tool",
  "author": "William Sharp @wsharp07",
  "bin": "./bin/run",
  "homepage": "https://github.com/wsharp07/muto",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "wsharp07/muto",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@databases/mysql": "^6.0.0",
    "@databases/pg": "^5.4.1",
    "@oclif/core": "^2.9.4",
    "@oclif/plugin-help": "^5.2.14",
    "@oclif/plugin-plugins": "^3.1.7"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.31",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.2",
    "chai": "^4.3.7",
    "mocha": "^10",
    "oclif": "^3.9.1",
    "prettier": "3.0.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "xo": "^0.55.0"
  },
  "oclif": {
    "bin": "muto",
    "dirname": "muto",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "dev": "npm run build && ./bin/dev",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "xo",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bugs": "https://github.com/wsharp07/muto/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
